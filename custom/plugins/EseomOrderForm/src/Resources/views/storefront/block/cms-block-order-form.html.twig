            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="eseom-order-form-loading-modal-label">{{ "eseom-order-form.loadingModal.headline" | trans | sw_sanitize}}</h5>
                </div>
                <div class="modal-body">
                    <div class="d-flex justify-content-center">
                        <div class="spinner-border" style="width: 4rem; height: 4rem;" role="status">
                            <span class="visually-hidden">Loading...</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="eseom-order-form-success-modal" tabindex="-1" role="dialog" aria-labelledby="eseom-order-form-success-modal-label" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="eseom-order-form-success-modal-label">{{ "eseom-order-form.successModal.headline" | trans | sw_sanitize}}</h5>
                    <button type="button" class="btn btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">{{ "eseom-order-form.successModal.continueShoppingBtn" | trans | sw_sanitize}}</button>
                    <a href="{{ path('frontend.checkout.cart.page') }}" class="btn btn-primary">{{ "eseom-order-form.successModal.toTheCartBtn" | trans | sw_sanitize}}</a>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="eseom-order-form-info-modal" tabindex="-1" role="dialog" aria-labelledby="eseom-order-form-info-modal-label" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="eseom-order-form-info-modal-label">{{ "eseom-order-form.infoModal.headline" | trans | sw_sanitize}}</h5>
                    <button type="button" class="btn btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">{{ "eseom-order-form.infoModal.closeBtn" | trans | sw_sanitize}}</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="eseom-order-form-error-modal" tabindex="-1" role="dialog" aria-labelledby="eseom-order-form-error-modal-label" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="eseom-order-form-error-modal-label">{{ "eseom-order-form.errorModal.headline" | trans | sw_sanitize}}</h5>
                    <button type="button" class="btn btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">{{ "eseom-order-form.errorModal.closeBtn" | trans | sw_sanitize}}</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="eseom-order-form-choose-variant-modal" tabindex="-1" role="dialog" aria-labelledby="eseom-order-form-choose-variant-modal-label" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="eseom-order-form-choose-variant-modal-label">{{ "eseom-order-form.chooseVariantModal.headline" | trans | sw_sanitize}}</h5>
                    <button type="button" class="btn btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">{{ "eseom-order-form.chooseVariantModal.closeBtn" | trans | sw_sanitize}}</button>
                </div>
            </div>
        </div>
    </div>
    {% endif %}
