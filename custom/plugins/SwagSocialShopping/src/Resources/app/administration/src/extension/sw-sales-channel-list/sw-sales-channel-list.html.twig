{% block sw_sales_channel_list_grid_column_name %}
    <template #column-name="{ item }">
        <sw-icon
            :name="getIconName(item)"
            size="18px"
        />
        <router-link
            :to="{
                name: 'sw.sales.channel.detail',
                params: { id: item.id }
            }"
        >
            {{ item.translated.name || item.name }}
        </router-link>
    </template>
{% endblock %}
