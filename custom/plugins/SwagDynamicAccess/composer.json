{
    "name": "swag/dynamic-access",
    "description": "Dynamic Access for Shopware 6",
    "version": "2.1.0",
    "type": "shopware-platform-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopware"
        }
    ],
    "require": {
        "shopware/core": "~6.5@dev",
        "shopware/storefront": "~6.5@dev"
    },
    "extra": {
        "shopware-plugin-class": "Swag\\DynamicAccess\\SwagDynamicAccess",
        "copyright": "(c) by shopware AG",
        "label": {
            "de-DE": "Dynamic Access",
            "en-GB": "Dynamic Access"
        },
        "description": {
            "de-DE": "Verwenden Sie den Rules mit dem Rule Builder, um zu definieren, welche Kunden Zugriff auf welche Produkte, Kategorien und auch Landing Pages haben sollen.",
            "en-GB": "Use Rules of the Rule Builder to define which customers have access to which products, categories and landing pages of your very online store. Configurable for each Sales Channel."
        },
        "manufacturerLink": {
            "de-DE": "https://store.shopware.com/shopware-ag.html",
            "en-GB": "https://store.shopware.com/en/shopware-ag.html"
        },
        "supportLink": {
            "de-DE": "https://issues.shopware.com/",
            "en-GB": "https://issues.shopware.com/"
        }
    },
    "autoload": {
        "psr-4": {
            "Swag\\DynamicAccess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\DynamicAccess\\Test\\": "tests/"
        }
    },
    "suggest": {
        "shopware/administration": "Require '*'. Allows you to to set up the plugin easily"
    },
    "conflict": {
        "shopware/storefront": "<6,>=7",
        "shopware/administration": "<6,>=7"
    },
    "repositories": {
        "platform-packages": {
            "type": "path",
            "url": "../../../src/*"
        }
    }
}
