{% block swag_customized_products_option_type_template_fileupload_template_file %}
    <template class="customized-products-upload-files-element small">

        {% block swag_customized_products_option_type_template_fileupload_template_file_container %}
            <div class="customized-products-upload-files-element__container">

                {% block swag_customized_products_option_type_template_fileupload_template_file_container_icon %}
                    <div class="customized-products-upload-files-element__container-icon">

                        {% block swag_customized_products_option_type_template_fileupload_template_file_container_icon_success_content_icon %}
                            {% sw_icon 'file' style { 'size': 'xs' } %}
                        {% endblock %}

                    </div>
                {% endblock %}

                {% block swag_customized_products_option_type_template_fileupload_template_container_filename %}
                    <div class="customized-products-upload-files-element__container-filename"></div>
                {% endblock %}

                {% block swag_customized_products_option_type_template_fileupload_template_container_close %}
                    <div class="customized-products-upload-files-element__container-close">

                        {% block swag_customized_products_option_type_template_fileupload_template_container_close_button %}
                                <button class="customized-products-upload-files-element__container-close-button" tabindex="-1">
                                    {% sw_icon 'x' style { 'pack': 'solid' } %}
                                </button>
                        {% endblock %}

                    </div>
                {% endblock %}

            </div>
        {% endblock %}

        {% block swag_customized_products_option_type_template_fileupload_template_container_error %}
            <div class="customized-products-error-subtitle"></div>
        {% endblock %}

    </template>
{% endblock %}
