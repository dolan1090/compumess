<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Swag\CustomizedProducts\Template\SalesChannel\TemplateListRoute" public="true">
            <argument type="service" id="sales_channel.swag_customized_products_template.repository"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateConfiguration\SalesChannel\TemplateConfigurationListRoute" public="true">
            <argument type="service" id="sales_channel.swag_customized_products_template_configuration.repository"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOption\SalesChannel\TemplateOptionListRoute" public="true">
            <argument type="service" id="sales_channel.swag_customized_products_template_option.repository"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOptionValue\SalesChannel\TemplateOptionValueListRoute" public="true">
            <argument type="service" id="sales_channel.swag_customized_products_template_option_value.repository"/>
        </service>

    </services>
</container>
