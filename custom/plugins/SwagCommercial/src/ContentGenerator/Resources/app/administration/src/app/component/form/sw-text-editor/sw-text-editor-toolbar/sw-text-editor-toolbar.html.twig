{% block sw_text_editor_toolbar_buttons_left %}
    {% parent %}

    <sw-text-editor-toolbar-button
        v-if="getLicense('CONTENT_GENERATOR-5780304') && isAICopilot && isInlineEdit"
        v-for="button in AIButton"
        :key="button.type"
        :is-inline-edit="isInlineEdit"
        :button-config="setButtonValues(button)"
        @button-click="onClickAIButton"
    >
        <template #buttonSlot>
            <sw-icon name="solid-sparkles" size="14" />
        </template>
    </sw-text-editor-toolbar-button>
{% endblock %}
