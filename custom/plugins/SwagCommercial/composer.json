{
    "name": "shopware/commercial",
    "description": "Commercial Features for Shopware 6",
    "type": "shopware-platform-plugin",
    "version": "5.6.3",
    "authors": [
        {
            "name": "shopware AG",
            "homepage": "https://shopware.com"
        }
    ],
    "require": {
        "shopware/core": "~v6.5.6",
        "symfony/expression-language": "~6.2.5 || ~6.3.0"
    },
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Shopware\\Commercial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shopware\\Commercial\\Tests\\": "tests/",
            "Shopware\\Commercial\\Tests\\Migration\\": "tests/migration/",
            "Shopware\\Commercial\\Tests\\Bench\\": "tests/performance/bench/",
            "Shopware\\Commercial\\Tests\\Unit\\": "tests/unit/php/",
            "Shopware\\Commercial\\Tests\\Integration\\": "tests/integration/php/"
        }
    },
    "extra": {
        "shopware-plugin-class": "Shopware\\Commercial\\SwagCommercial",
        "copyright": "shopware AG",
        "label": {
            "de-DE": "Shopware Commercial",
            "en-GB": "Shopware Commercial"
        },
        "description": {
            "de-DE": "Diese Erweiterung beinhaltet alle Kommerziellen Features aus der Professional / Enterprise Version. Features werden mit der zugeh√∂rigen Lizenz aktiviert",
            "en-GB": "This extension contains all commercial features from the professional or enterprise version. Features are activated by the correct license................."
        },
        "manufacturerLink": {
            "de-DE": "https://shopware.com",
            "en-GB": "https://shopware.com"
        },
        "supportLink": {
            "de-DE": "https://shopware.com",
            "en-GB": "https://shopware.com"
        }
    },
    "scripts": {
        "lint": [
            "@ecs-fix",
            "@phpstan",
            "@phpunit"
        ],
        "ecs": "../../../vendor/bin/ecs check src tests",
        "ecs-fix": "../../../vendor/bin/ecs check --fix src tests",
        "phpunit": "../../../vendor/bin/phpunit",
        "phpstan": [
            "php tests/phpstan/bootstrap.php",
            "../../../vendor/bin/phpstan analyze"
        ],
        "phpbench": [
            "@putenv ENABLE_COMMERCIAL=true",
            "../../../vendor/bin/phpbench run --report=compressed"
        ],
        "bc-check": [
            "@php ../../../vendor-bin/roave-backward-compatibility-check/bin/verify-version.php",
            "../../../vendor/bin/roave-backward-compatibility-check --from=$(git tag -l --sort -version:refname | head -n 1)"
        ],
        "npm:jest:admin": [
            "export ADMIN_PATH=\"$PWD/../../../src/Administration/Resources/app/administration\"; npm --prefix $PWD/tests/jest/administration"
        ],
        "jest:init": [
            "npm --prefix $PWD/tests/jest/storefront clean-install --no-audit --prefer-offline",
            "npm --prefix $PWD/tests/jest/administration clean-install --no-audit --prefer-offline"
        ],
        "jest:admin": [
            "@npm:jest:admin run unit"
        ],
        "jest:admin:watch": [
            "Composer\\Config::disableProcessTimeout",
            "@npm:jest:admin run unit-watch"
        ],
        "jest:storefront": [
            "export STOREFRONT_PATH=\"$PWD/../../../src/Storefront/Resources/app/storefront\"; npm --prefix $PWD/tests/jest/storefront run unit"
        ],
        "jest:storefront:watch": [
            "Composer\\Config::disableProcessTimeout",
            "export STOREFRONT_PATH=\"$PWD/../../../src/Storefront/Resources/app/storefront\"; npm --prefix $PWD/tests/jest/storefront run unit-watch"
        ]
    },
    "scripts-descriptions": {
        "lint": "Runs all linting tools"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
