{% sw_extends '@Storefront/storefront/utilities/offcanvas.html.twig' %}

{% set productSeries = page.product.translated.customFields.migration_Compumess57Live_product_series %}
{% set productMarke = page.product.translated.customFields.migration_Compumess57Live_product_marke %}
{% set productLeistungnum = page.product.translated.customFields.migration_Compumess57Live_product_leistungnum %}
{% set productAusgangsleistung = page.product.translated.customFields.migration_Compumess57Live_product_attr2 %}
{% set productAusgangsspannung = page.product.translated.customFields.migration_Compumess57Live_product_ausgangsspannung %}
{% set productAusgangsstrom = page.product.translated.customFields.migration_Compumess57Live_product_attr4 %}
{% set productAusgange = page.product.translated.customFields.migration_Compumess57Live_product_attr6 %}
{% set productEinAusgangsisolation = page.product.translated.customFields.migration_Compumess57Live_product_attr9 %}
{% set productMTBF = page.product.translated.customFields.migration_Compumess57Live_product_attr4 %}
{% set productTyp = page.product.translated.customFields.migration_Compumess57Live_product_typ %}
{% set productBauform = page.product.translated.customFields.migration_Compumess57Live_product_bauform %}
{% set productEingang = page.product.translated.customFields.migration_Compumess57Live_product_eingang %}
{% set productKategorie = page.product.translated.customFields.migration_Compumess57Live_product_kategorie %}
{% set productSchnittstellen = page.product.translated.customFields.migration_Compumess57Live_product_schnittstellen %}
{% set productBeschreibung = page.product.translated.customFields.migration_Compumess57Live_product_bullet_point %}
{% set productEingangsbereich = page.product.translated.customFields.migration_Compumess57Live_product_eingangsbereich %}
{% set productArbeitstemperatur = page.product.translated.customFields.migration_Compumess57Live_product_arbeitstemperatur %}
{% set productRegelung = page.product.translated.customFields.migration_Compumess57Live_product_regelung %}
{% set productSpezielleZulassungen = page.product.translated.customFields.migration_Compumess57Live_product_spezielle_zulassungen %}
{% set productAbmessungenLange = page.product.translated.customFields.migration_Compumess57Live_product_abmessungen_l_nge %}
{% set productAbmessungenBreite = page.product.translated.customFields.migration_Compumess57Live_product_abmessungen_breite %}
{% set productAbmessungenHohe = page.product.translated.customFields.migration_Compumess57Live_product_abmessungen_h_he %}
{% set productSchaltfrequenzTyp = page.product.translated.customFields.migration_Compumess57Live_product_schaltfrequenz_typ %}

{% block utilities_offcanvas_content %}
    <div class="content--description">
        <table class="product--properties-table">
            <tbody>
                <tr>
                    <td style=" background: none; ">
                        {{ page.product.translated.customFields.migration_Compumess57Live_product_header_description|raw }}
                    </td>
                </tr>
            </tbody>
        </table>
        <br/>
        <div class="product--properties panel has--border">
            <table class="product--properties-table">
                <tbody>
                    {% if productSeries %}
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Serie
                            </td>
                            <td class="product--properties-value">
                                {{ productSeries }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productMarke %}
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Marke
                            </td>
                            <td class="product--properties-value">
                                {{ productMarke }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productLeistungnum %}
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                LeistungNum
                            </td>
                            <td class="product--properties-value">
                                {{ productLeistungnum }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productAusgangsleistung %}
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Ausgangsleistung
                            </td>
                            <td class="product--properties-value">
                                {{ productAusgangsleistung }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productAusgangsspannung %}
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Ausgangsspannung
                            </td>
                            <td class="product--properties-value">
                                {{ productAusgangsspannung }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productAusgangsstrom %}
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Ausgangsstrom
                            </td>
                            <td class="product--properties-value">
                                {{ productAusgangsstrom }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productAusgange %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Ausgänge
                            </td>
                            <td class="product--properties-value">
                                {{ productAusgange }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productEinAusgangsisolation %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Ein-/Ausgangsisolation
                            </td>
                            <td class="product--properties-value">
                                {{ productEinAusgangsisolation }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productMTBF %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                MTBF≥
                            </td>
                            <td class="product--properties-value">
                                {{ productMTBF }}
                            </td>
                        </tr>
                    {% endif %}      
                    {% if productTyp %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Typ
                            </td>
                            <td class="product--properties-value">
                                {{ productTyp }}
                            </td>
                        </tr>
                    {% endif %}    
                    {% if productBauform %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Bauform
                            </td>
                            <td class="product--properties-value">
                                {{ productBauform }}
                            </td>
                        </tr>
                    {% endif %}   
                    {% if productEingang %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Eingang
                            </td>
                            <td class="product--properties-value">
                                {{ productEingang }}
                            </td>
                        </tr>
                    {% endif %}                                                                  
                    {% if productKategorie %}
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Kategorie
                            </td>
                            <td class="product--properties-value">
                                {{ productKategorie }}
                            </td>
                        </tr>
                    {% endif %}                    
                    {% if productSchnittstellen %}
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Schnittstellen
                            </td>
                            <td class="product--properties-value">
                                {{ productSchnittstellen }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productBeschreibung %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Beschreibung
                            </td>
                            <td class="product--properties-value">
                                {{ productBeschreibung|raw }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productEingangsbereich %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Eingangsbereich
                            </td>
                            <td class="product--properties-value">
                                {{ productEingangsbereich }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productArbeitstemperatur %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Arbeitstemperatur
                            </td>
                            <td class="product--properties-value">
                                {{ productArbeitstemperatur }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productRegelung %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Regelung
                            </td>
                            <td class="product--properties-value">
                                {{ productRegelung }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productSpezielleZulassungen %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Spezielle Zulassungen
                            </td>
                            <td class="product--properties-value">
                                {{ productSpezielleZulassungen }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productAbmessungenLange %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Abmessungen Länge
                            </td>
                            <td class="product--properties-value">
                                {{ productAbmessungenLange }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productAbmessungenBreite %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Abmessungen Breite
                            </td>
                            <td class="product--properties-value">
                                {{ productAbmessungenBreite }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productAbmessungenHohe %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Abmessungen Höhe
                            </td>
                            <td class="product--properties-value">
                                {{ productAbmessungenHohe|raw }}
                            </td>
                        </tr>
                    {% endif %}
                    {% if productSchaltfrequenzTyp %}                
                        <tr class="product--properties-row">
                            <td class="product--properties-label is--bold">
                                Schaltfrequenz typ.
                            </td>
                            <td class="product--properties-value">
                                {{ productSchaltfrequenzTyp }}
                            </td>
                        </tr>
                    {% endif %}                    
                </tbody>
            </table>
        </div>
    </div>
{% endblock %}